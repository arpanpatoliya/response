{
    "name": "arpanpatoliya/response",
    "description": "A simple Laravel package for standardized API responses",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "response", "api", "json"],
    "authors": [
        {
            "name": "Arpan",
            "email": "info.arpan08@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
         "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Arpanpatoliya\\Response\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arpanpatoliya\\Response\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arpanpatoliya\\Response\\Providers\\ResponseServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
